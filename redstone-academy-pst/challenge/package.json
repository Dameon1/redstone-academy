    "vue-router": "^3.2.0",
    "vue-spinner": "^1.0.4",
    "vue-toasted": "^1.1.28",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
