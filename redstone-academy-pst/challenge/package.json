    "eslint-plugin-vue": "^8.5.0",
    "jest": "^27.4.3",
    "node-sass": "^7.0.1",
    "prettier": "^2.2.1",
    "replace-in-file": "^6.2.0",
    "ts-jest": "^27.1.3",
